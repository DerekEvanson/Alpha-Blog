<h1>Articles listing page</h1>

<table>
    <thread>
    <tr>
        </tr>
        <th>Title</th>
        <th>Actions</th>
    </thread>

    <tbody>
        <% @articles.each do |article| %>
            <tr>
                <td><%= article.title %></td>
                <td><%= link_to 'Show', article_path(article.id), method: :get %></td>
                <td><%= link_to 'Delete', article_path(article.id), method: :delete %></td>
            </tr>
        <% end %>
    </tbody>
</table>